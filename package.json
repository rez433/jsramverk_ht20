{
  "name": "jsramverk-editor",
  "version": "2.2.1",
  "description": "Collaborative text editor build with MERN stack",
  "main": "index.js",
  "scripts": {
    "test": "jest ./backend --detectOpenHandles --coverage --config ./backend/jest.config.json  --ci --reporters=default --reporters=jest-Junit --testTimeout=10000",
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "postinstall": "(cd backend && npm i); (cd frontend && npm i)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rez433/jsramverk_ht20.git"
  },
  "keywords": [
    "Nodejs",
    "Reactjs",
    "Expressjs",
    "MongoDB",
    "Mongoose",
    "Jest",
    "SuperTest"
  ],
  "author": "Rezaee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rez433/jsramverk_ht20/issues"
  },
  "homepage": "https://github.com/rez433/jsramverk_ht20#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "mongoose": "^6.6.5",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^7.4.0",
    "jest": "^29.3.1",
    "jest-junit": "^15.0.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3"
  },
  "jest-junit": {
    "outputDirectory": "reports",
    "outputName": "jest-junit.xml",
    "ancestorSeparator": " â€º ",
    "uniqueOutputName": "false",
    "suiteNameTemplate": "{filepath}",
    "classNameTemplate": "{classname}",
    "titleTemplate": "{title}"
  }
}
