{
  "name": "jsramverk-editor",
  "version": "3.5.0",
  "description": "Collaborative text editor build with MERN stack",
  "main": "index.js",
  "scripts": {
    "test": "cd ./backend && jest --detectOpenHandles --coverage ",
    "test_front": "cd ./frontend && react-scripts test",
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "postinstall": "(cd backend && npm i); (cd frontend && npm i)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rez433/jsramverk_ht20.git"
  },
  "keywords": [
    "Nodejs",
    "Reactjs",
    "Expressjs",
    "MongoDB",
    "Mongoose",
    "Jest",
    "SuperTest"
  ],
  "author": "Rezaee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rez433/jsramverk_ht20/issues"
  },
  "homepage": "https://github.com/rez433/jsramverk_ht20#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "mongoose": "^6.6.5",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^7.4.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "react-scripts": "^5.0.1"
  }
}
