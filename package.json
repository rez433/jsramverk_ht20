{
  "name": "jsramverk-editor",
  "version": "2.4.0",
  "description": "Collaborative text editor build with MERN stack",
  "main": "index.js",
  "scripts": {
    "test": "cd ./backend && jest --detectOpenHandles --coverage ",
    "test_front": "cd ./frontend && react-scripts test",
    "start": "node backend/server.js",
    "server": "nodemon backend/server.js",
    "client": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "postinstall": "(cd backend && npm i); (cd frontend && npm i)"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rez433/jsramverk_ht20.git"
  },
  "keywords": [
    "Nodejs",
    "Reactjs",
    "Expressjs",
    "MongoDB",
    "Mongoose",
    "Jest",
    "SuperTest"
  ],
  "author": "Rezaee",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rez433/jsramverk_ht20/issues"
  },
  "homepage": "https://github.com/rez433/jsramverk_ht20#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "mongoose": "^6.6.5",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "concurrently": "^7.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.3.1",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^29.3.1",
    "jsdom": "^20.0.3",
    "msw": "^0.49.2",
    "nodemon": "^2.0.20",
    "react-scripts": "^5.0.1",
    "supertest": "^6.3.3"
  },
  "transform": {
    "\\.[jt]sx?$": "babel-jest",
    "\\.css$": "jest-css-modules-transform"
  }
}
